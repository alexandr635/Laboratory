<Window x:Class="Laboratory.Windows.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laboratory.Windows"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800"
        Loaded="AdminWindow_Load"
        Icon="../Resources/logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="206*"></RowDefinition>
            <RowDefinition Height="213*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid x:Name="adminGrid">
            <Image Margin="25,24,581,48" Name="adminImg"></Image>
        </Grid>
        <Button x:Name="showHistoryBtn" Content="Просмотри истории" Grid.Row="1" Margin="26,42,0,0" Width="229" Click="ShowHistoryBtn_Click"/>
        <Button x:Name="backBtn" Content="Назад" Grid.Row="1" Margin="26,119,0,0" Width="229" Click="BackBtn_Click"></Button>
        
    </Grid>
</Window>
